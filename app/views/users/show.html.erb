<main>
  <p></p>
  <%= render 'books/index', book: @book, user: @user %>
  <div>
    <h2>Users</h2>
    <table>
      <thead>
        <tr>
          <th>image</th>
          <th>name</th>
          <th colspan="3"></th>
        </tr>
      </thead>
      <tbody>
       <%= @users.each do |user| %>
        <tr>
          <td><%= image_tag @user.get_profile_image(100, 100) %></td>
          <td><%= user.name %></td>
          <td><%= link_to "Show", book_path(@book.id) %></td>
        </tr>
       <% end %>
      </tbody>
    </table>
  </div>
</main>
